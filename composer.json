{
    "name": "issam-elkhadir/laravel-query-macros",
    "description": "A collection of useful, production-ready Eloquent query macros for Laravel",
    "keywords": [
        "laravel",
        "eloquent",
        "query",
        "macros",
        "database",
        "search",
        "json",
        "like"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Issam Elkhadir",
            "email": "issamelkhader55@gmail.com",
            "homepage": "https://github.com/issamElkhadirYC",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/issamElkhadirYC/laravel-query-macros",
    "support": {
        "issues": "https://github.com/issamElkhadirYC/laravel-query-macros/issues",
        "source": "https://github.com/issamElkhadirYC/laravel-query-macros"
    },
    "require": {
        "php": "^8.2",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelQueryMacros\\QueryMacros\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelQueryMacros\\QueryMacros\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelQueryMacros\\QueryMacros\\QueryMacrosServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}